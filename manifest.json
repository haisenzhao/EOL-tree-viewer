{
	"name": "EOL tree lightbox",
	"version": "0.0.0.1",
	"description": "Uses the JIT to visualize EOL classification in a lightbox",
	"background_page": "background.html",
	"permissions": [
		"tabs",
		"http://*.eol.org/",
		"http://eol.org/"
	],
	"content_scripts": [
    {
      "matches": ["http://*.eol.org/pages/*"],
      "js": ["js/jquery-1.3.2.js", "js/jquery.DOMWindow.js", "js/Jit/jit.js", "js/prepdom.js", "js/flashxmltree.js", "js/texthtmltree.js", "js/vis.js", "js/test.js"],
	  "css": ["css/Treemap.css"]
    }
  ]
}